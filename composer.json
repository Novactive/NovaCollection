{
    "name": "novactive/collection",
    "description": "++ Collection ++ Abstaction library for PHP. ",
    "homepage": "https://github.com/Novactive/NovaCollection",
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "SÃ©bastien Morel",
            "homepage": "https://github.com/Plopix"
        },
        {
            "name": "Luke Visonini",
            "homepage": "https://github.com/nozavroni"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jpgraph/jpgraph",
                "version": "4.0.2",
                "dist": {
                    "url": "http://jpgraph.net/download/download.php?p=11",
                    "type": "tar"
                }
            }
        }
    ],
    "support": {
        "issues": "https://github.com/Novactive/NovaCollection/issues"
    },
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.1",
        "squizlabs/php_codesniffer": "^2.7",
        "phpmd/phpmd": "^2.4",
        "fzaninotto/Faker": "dev-master",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.7",
        "jpgraph/jpgraph": "4.0.2",
        "phpdocumentor/reflection-docblock": "^3.1",
        "doctrine/orm": "^2.5",
        "doctrine/data-fixtures": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Novactive\\Collection\\": "src/",
            "Novactive\\Tests\\": "tests/Tests",
            "Novactive\\Tests\\Perfs\\": "tests/Perfs",
            "Novactive\\Tests\\Doctrine\\": "tests/Doctrine"
        },
        "classmap": [
            "src/Doctrine/ArrayCollection.php",
            "src/Doctrine/Collection.php"
        ]
    },
    "autoload-dev": {
        "files": [
            "tests/functions.php",
            "tests/Perfs/jpgraphloader.php"
        ]
    },
    "config": {
        "secure-http": false
    }
}
